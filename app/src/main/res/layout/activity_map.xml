                android:background="@drawable/ic_edittext_background"
                android:textSize="24dp"
                android:textStyle="bold"
                android:gravity="center" />

            <TextView
                android:id="@+id/keyTextView8"
                android:layout_width="50dp"
                android:layout_height="50dp"
